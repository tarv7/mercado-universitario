<% reviews.each do |review| %>
  <div class="col-2">
    <%= image_tag 'https://www.drdavidludwig.com/wp-content/uploads/2017/01/1-RIS_6IbCLYv1X3bzYW1lmA.jpeg', class: 'profile-pic-rounded' %>
  </div>

  <div class="col-10">
    <div class="row header">
      <h3><%= review.user.name %></h3>
      <span>
        <%= image_tag 'star.svg', class: 'icon-star' %>
        <%= review.value %>
      </span>
    </div>
    <div class="row">
      <p><%= review.comment %></p>
    </div>
  </div>
<% end %>